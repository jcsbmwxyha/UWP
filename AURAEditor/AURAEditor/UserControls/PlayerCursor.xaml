<UserControl
    x:Class="AuraEditor.UserControls.PlayerCursor"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:AuraEditor.UserControls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="200"
    d:DesignWidth="30"
    ManipulationMode="TranslateX"
    ManipulationStarted="PlayerCursor_ManipulationStarted"
    ManipulationDelta="PlayerCursor_ManipulationDelta"
    PointerEntered="PlayerCursor_PointerEntered"
    PointerExited="PlayerCursor_PointerExited">

    <Image Source="ms-appx:///Assets/Layer/asus_auraeditor_timeline_ic.png" HorizontalAlignment="Left"/>
</UserControl>
